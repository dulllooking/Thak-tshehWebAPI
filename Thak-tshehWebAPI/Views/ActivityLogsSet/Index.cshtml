@model IEnumerable<Thak_tshehWebAPI.Models.ActivityLog>

@{
    ViewBag.Title = "Index";
}

<h2>活動報名記錄</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Activity.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Activity.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Account)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActivityPrice)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrderState)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserMobilePhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreatDate)
        </th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Activity.Id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Activity.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.User.Id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.User.Account)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ActivityPrice)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.OrderState)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.UserName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.UserMobilePhone)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.CreatDate)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
